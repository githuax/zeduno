# ZedUno Frontend Environment Variables

# API Configuration
# For development (default if not set)
# VITE_API_URL=http://localhost:5000/api

# For production with domain
# VITE_API_URL=https://yourdomain.com/api

# For production with IP
# VITE_API_URL=http://YOUR_SERVER_IP/api

# App Configuration
VITE_APP_NAME=ZedUno
VITE_APP_VERSION=1.0.0

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PAYMENT_GATEWAYS=true
VITE_DEFAULT_CURRENCY=KES

# Development
VITE_DEBUG=false
VITE_LOG_LEVEL=warn